<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.HabitatController" prefHeight="180.0" prefWidth="200.0">
    <children>
        <TableView layoutX="17.0" layoutY="47.0" prefHeight="308.0" prefWidth="567.0" fx:id="habitatTableView">
            <columns>
                <TableColumn prefWidth="38.0" text="ID" fx:id="habitat_IdColumn" />
                <TableColumn prefWidth="136.0" text="Name" fx:id="habitat_NameColumn" />
                <TableColumn prefWidth="335.0" text="Description" fx:id="habitat_DescriptionColumn" />
                <TableColumn minWidth="0.0" prefWidth="57.0" text="Region ID" fx:id="habitat_regionIdColumn" />
            </columns>
        </TableView>
        <Button layoutX="17.0" layoutY="14.0" mnemonicParsing="false" text="Add" fx:id="habitat_addButton" onAction="#addHabitat"/>
    </children>
</AnchorPane>
