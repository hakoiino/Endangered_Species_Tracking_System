         <children>
            <Text layoutX="2.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Name:" wrappingWidth="143.99999999999994" />
            <TextField fx:id="name_input" layoutY="31.0" />
            <Text layoutX="3.0" layoutY="102.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Description" wrappingWidth="143.99999999999994" />
            <TextField fx:id="description_input" layoutX="1.0" layoutY="106.0" />
            <Text layoutX="1.0" layoutY="177.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Region ID:" wrappingWidth="143.99999999999994" />
