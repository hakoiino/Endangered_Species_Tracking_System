            <columns>
                <TableColumn fx:id="habitat_IdColumn" prefWidth="38.0" text="ID" />
                <TableColumn fx:id="habitat_NameColumn" prefWidth="136.0" text="Name" />
                <TableColumn fx:id="habitat_DescriptionColumn" prefWidth="335.0" text="Description" />
